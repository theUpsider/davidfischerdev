{
  "name": "material-ui-vite-ts",
  "private": true,
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "build-prod": "tsc && vite build --mode production",
    "build-prod-legacy": "NODE_OPTIONS=\"--openssl-legacy-provider --max-old-space-size=4096\" npm run build-prod",
    "build-dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "docker:build": "docker build -t davidfischerdev-frontend .",
    "docker:build-prod": "docker build -t davidfischerdev-frontend --build-arg VITE_API_BASE_URL=https://api.davidfischer.dev --build-arg NODE_ENV=production .",
    "docker:run": "docker run -p 3000:80 davidfischerdev-frontend",
    "docker:pull": "docker pull ghcr.io/theUpsider/davidfischerdev:latest",
    "docker:run-ghcr": "docker run -p 80:80 ghcr.io/theUpsider/davidfischerdev:latest",
    "docker:compose:up": "docker-compose up --build",
    "docker:compose:up-dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:compose:up-ghcr": "docker-compose -f docker-compose.prod.yml up"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.5",
    "axios": "^1.7.2",
    "normalize.css": "^8.0.1",
    "react": "latest",
    "react-dom": "latest",
    "react-router-dom": "^6.21.1",
    "sitemap": "^8.0.0"
  },
  "devDependencies": {
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "latest",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-prefer-arrow-functions": "^3.2.4",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "typescript": "latest",
    "vite": "latest"
  }
}